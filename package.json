{
  "name": "api-service",
  "version": "1.0.0",
  "description": "api service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "#npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t api-service .",
    "docker:run": "docker run -p 3000:3000 -d api-service",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start2": "node -r source-map-support/register index2",
    "debug": "nodemon -r source-map-support/register --inspect=0.0.0.0 .",
    "prepublishOnly": "npm run test",
    "preinstall": "npx npm-force-resolutions"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.7.4",
    "@loopback/context": "^2.1.1",
    "@loopback/core": "^1.12.4",
    "@loopback/openapi-v3": "^2.0.0",
    "@loopback/repository": "^1.19.1",
    "@loopback/rest": "^2.0.0",
    "@loopback/rest-explorer": "^1.4.10",
    "@loopback/service-proxy": "^1.3.17",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "@panosc-portal/panosc-common-ts": "^0.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.1",
    "source-map-support": "^0.5.16",
    "@loopback/testlab": "^1.10.3",
    "@types/node": "^10.17.14",
    "typescript": "~3.7.5",
    "nodemon": "^2.0.2"
  },
  "resolutions": {
    "minimist": ">=1.2.2"
  }
}
